rm -f /var/tmp/x.txt; echo "digraph tenacitas { " > /var/tmp/dot.txt; find . \( -name "*.h" -o -name "*.cpp" \) | xargs grep "$include <tenacitas" | while read a; do l0=`echo "$a" | cut -d : -f1`; l1=`echo "$l0" | cut -d '/' -f 2`; l2=`echo "$a" | cut -d : -f 2`; l3=`echo "$l2" | cut -d '<' -f 2`; l4=`echo "$l3" | cut -d '/' -f 1`;  if [ "$l1" != "$l4" ]; then echo "\"$l1\" -> \"$l4\"" >> /var/tmp/x.txt ; fi; done ; sort -u /var/tmp/x.txt >> /var/tmp/dot.txt; echo "}" >> /var/tmp/dot.txt
